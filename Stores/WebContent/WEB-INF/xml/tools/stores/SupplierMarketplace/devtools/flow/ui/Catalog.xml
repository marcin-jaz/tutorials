	<panel id="CatalogPanel" title="CatalogPanel.title" description="CatalogPanel.description" display-priority="3"/>

	<option-group id="Search.checkbox" type="checkbox" display-priority="1" panel="CatalogPanel" >
		
		<option id="Search.checkboxEnable"
			label="Search.checkboxEnable.label"
			description="Search.checkboxEnable.description"
			enables-features="catalogSearch" />
						
	</option-group>
		
	<option-group id="ProductDisplay.checkbox" type="checkbox" display-priority="2" panel="CatalogPanel" >
		
