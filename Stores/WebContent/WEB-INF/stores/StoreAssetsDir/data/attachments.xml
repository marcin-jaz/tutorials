<!-- Attachments -->

<!-- New Attachment feature does NOT use ATTACHMENT or ATTACHUSG tables -->
<!-- Valid Attachment Object Types are CATALOG, CATGROUP, CATENTRY, CALLATERAL -->
<!-- Valid Attachment Relations are No Usages Selected, Warranty, Images, Full Images, User Manual -->

<!-- Attachments #1 will be the category image used by the mobile store   -->



<!-- Resolve the Object Types that will be referenced
<atchobjtyp
   atchobjtyp_id="@atchobjtyp_id_CATENTRY"
   identifier="CATENTRY"
/>
-->


<!-- Holds the usage information of attachment relations -->

<atchrlus
   atchrlus_id="@atchrlus_id_Mobile"
   identifier="MOBILE"
/>


<!--               -->
<!-- Attachment #1 -->
<!--               -->

<!-- Holds information about the target, which is used to hold a collection of assets -->
<atchtgt
   atchtgt_id="@atchtgt_id_mobilecategory_1"
   storeent_id="&TARGET_STORE_ID;"
   member_id="&CAT_MEMBER_ID;"
   identifier="furniture category image used by mobile page"
/>

<!-- Attachment relation between a business object and an attachment target -->
<catgroup
   catgroup_id="@catgroup_id_furniture"
   member_id="&CAT_MEMBER_ID;"
   identifier="Furniture"
   markfordelete="0"
>
	<ISA>
		<atchrel
		  atchrel_id="@atchrel_id_mobilecategory_1"
		  object_id="@catgroup_id_furniture"
		  atchobjtyp_id="2"
		  atchtgt_id="@atchtgt_id_mobilecategory_1"
		  atchrlus_id="@atchrlus_id_Mobile"
		/>
	</ISA>
</catgroup>

<!-- Holds the meta data for the attachment asset in the attachment target -->
<atchast
  atchast_id="@atchast_id_mobilecategory_1"
  storeent_id="&TARGET_STORE_ID;"
  atchtgt_id="@atchtgt_id_mobilecategory_1"
  atchastpath="mobile/images/index_banner.jpg"
  directorypath="mobile/images/"
  mimetype="image/jpeg"
/>

<cmfiledir cmfiledir_id="@cmfiledir_id_dir_1" identifier="&STORE_DIRECTORY_LOWERCASE;" displayname="&STORE_DIR;"/>
<cmfiledir cmfiledir_id="@cmfiledir_id_dir_2" cmfiledir_parent="@cmfiledir_id_dir_1" identifier="attachment" displayname="Attachment"/>

<cmfile cmfile_id="@cmfile_id_mobilecategory_1" cmfilepath="/&STORE_DIR;/mobile/images/index_banner.jpg" promote_fs="1" upload_timestamp="2007-10-01 16:00:00.001" promote_timestamp="2007-10-01 16:00:00.001" associated="1" storage_type="-1" cmfiledir_id="@cmfiledir_id_dir_2"/>
