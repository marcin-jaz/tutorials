<Definitions>    

	<PrimaryObjectDefinition definitionName="plmBasePageLayoutSalesCatalogPrimaryObjectDefinition"
		isBaseDefinition="true"
		detailsViewHeaderText="${catalogResources.UtilityPaneHeaderText}"
		displayName="${catalogResources.salesCatalog_DisplayName}"
		displayNameProperty="identifier"
		helpLink="concepts/cpzpagecomptool.htm"
		idProperty="catalogId" 
		newDisplayName="${catalogResources.salesCatalog_NewDisplayName}"
		objectGroups="PageLayoutSalesCatalogAlias"
		utilitiesListDefinitionName="plmUtilitiesBrowseGrid"
		writeUsage="IBM_ManagePageComposerTool"
		searchType="FindLayoutCatalogs">

		<!--- 
			Retrieves the {@link plmChildSalesCatalogGroupBrowsingPage}, 
			{@link plmSharedSalesCatalogGroupBrowsingPage}, {@link plmSharedInheritedSalesCatalogGroupBrowsingPage},
			{@link plmSharedCatalogGroupBrowsingPage} and {@link plmSharedInheritedCatalogGroupBrowsingPage} child objects.
		-->			
		<GetChildrenService objectTypes="ChildCatalogGroupBrowsingPage,SharedCatalogGroupBrowsingPage,SharedInheritedCatalogGroupBrowsingPage"
			url="/cmc/GetPageLayoutSalesCatalogChildren">
			<ServiceParam name="storeId"/>
			<ServiceParam name="assetStoreId"/>
			<ServiceParam name="defaultLanguageId"/>
			<ServiceParam name="identifier" propertyName="identifier"/>
			<ServiceParam name="catalogStoreId" propertyName="objectStoreId"/>
			<ServiceParam checkObjectDefinition="true" name="catalogType" propertyName="objectType"/>
		</GetChildrenService>
		
		<!---  
		    Reference definition for a child Sales category browsing page.
		-->			
		<CollectionReferenceObjectDefinition baseDefinitionName="plmChildCatalogGroupBrowsingPage"/>
		
		<!---  
		    Reference definition for a shared child category browsing page
		-->	
		<ReferenceObjectDefinition baseDefinitionName="plmSharedCatalogGroupBrowsingPage"/>

		<!---  
		    Reference definition for a shared child Sales category browsing page
		-->		
<!--		<ReferenceObjectDefinition baseDefinitionName="plmSharedSalesCatalogGroupBrowsingPage"/>-->

		<!---
			Reference definition for a shared child inherited category browsing page
		-->
		<ReferenceObjectDefinition baseDefinitionName="plmSharedInheritedCatalogGroupBrowsingPage"/>

		<!---
			Reference definition for a shared child inherited Sales category browsing page
		-->
<!--		<ReferenceObjectDefinition baseDefinitionName="plmSharedInheritedSalesCatalogGroupBrowsingPage"/>-->

		<!--- This navigation list definition is used to display
			{@link plmChildSalesCatalogGroupBrowsingPage}, 
			{@link plmSharedSalesCatalogGroupBrowsingPage}, {@link plmSharedInheritedSalesCatalogGroupBrowsingPage},
			{@link plmSharedCatalogGroupBrowsingPage} and {@link plmSharedInheritedCatalogGroupBrowsingPage}.
		-->
		<!--<NavigationListDefinition name="SalesCatalogBrowsingPageChildList"
			displayName="${plmPageLayoutResources.salesCategoryPagesList}"
			isDefault="true"
			listDefinitionName="plmSalesCategoryBrowsingPageChildList"
			listTitle="${plmPageLayoutResources.salesCategoryPagesList}"
			helpLink="concepts/cpzpagecomptool.htm"/>
			
		-->
		<NavigationListDefinition name="CategoryBrowsingPageChildList"
			displayName="${plmPageLayoutResources.categoryPagesList}"
			isDefault="true" 
			listDefinitionName="plmCategoryBrowsingPageChildList" 
			listTitle="${plmPageLayoutResources.categoryPagesList}" 
			
			helpLink="concepts/cpzpagecomptool.htm"/>
		
	</PrimaryObjectDefinition>

	<PrimaryObjectDefinition definitionName="PageLayoutSalesCatalog"
		baseDefinitionName="plmBasePageLayoutSalesCatalogPrimaryObjectDefinition"
		objectType="PageLayoutSalesCatalog">
	</PrimaryObjectDefinition>
	
	<PrimaryObjectDefinition definitionName="InheritedPageLayoutSalesCatalog"
		baseDefinitionName="plmBasePageLayoutSalesCatalogPrimaryObjectDefinition"
		objectType="InheritedPageLayoutSalesCatalog"
		displayName="${catalogResources.inheritedSalesCatalog_DisplayName}">
 	</PrimaryObjectDefinition>
 	
</Definitions>