<Definitions>

	<!--- This definition defines the base object from which PredDynaKitBrowsingPage and InheritedPredDynaKitBrowsingPage are derived.
	-->
	<PrimaryObjectDefinition definitionName="plmBasePredDynaKitBrowsingPagePrimaryObjectDefinition" 
		baseDefinitionName="plmBaseCatalogEntryBrowsingPagePrimaryObjectDefinition"
		isBaseDefinition="true" 
		detailsViewHeaderText="${catalogResources.UtilityPaneHeaderText}" 
		displayName="${plmPageLayoutResources.predDynaKitPage_DisplayName}"
	    helpLink="concepts/cpzpagecomptool.htm">
	    
		<!--- This navigation list definition is used to display the layout assignments for this catalog entry page. -->
		<NavigationListDefinition name="CatalogEntryBrowsingPageAssignmentChildList"
			displayName="${plmPageLayoutResources.navigation_layoutAssignmentListPrompt}"
		 	isDefault="true"
		 	listDefinitionName="plmPageLayoutReferenceList" 
		 	toolbarIcon="listToolbarIcon"
		 	listTitle="${plmPageLayoutResources.navigation_layoutAssignmentPrompt}" />	

		<!--- This navigation list definition is used to display the layout assignments for this catalog entry page as a calendar view. -->
		<NavigationListDefinition name="CatalogEntryBrowsingPageAssignmentGanttChildList"
			displayName="${plmPageLayoutResources.pageLayoutsGanttChartMenuTitle}"
		 	listDefinitionName="plmPageLayoutGanttReferenceList" 
		 	listTitle="${plmPageLayoutResources.pageLayoutsGanttChartTitle}"
		 	toolbarIcon="ganttChartToolbarIcon" />	
		
	</PrimaryObjectDefinition>

	<!---		
		This definition represents the primary object definition describing a Predefined Dynamic Kit Page as a business object.
	-->
    <PrimaryObjectDefinition definitionName="PredDynaKitBrowsingPage"
    	objectType="PredDynaKitBrowsingPage"
    	baseDefinitionName="plmBasePredDynaKitBrowsingPagePrimaryObjectDefinition"
     	headerIcon="kitPageHeaderIcon"
		icon="kitPageIcon">
	</PrimaryObjectDefinition>

	<!---		
		This definition represents the primary object definition describing an Inherited Predefined Dynamic Kit Page as a business object.
	-->
    <PrimaryObjectDefinition definitionName="InheritedPredDynaKitBrowsingPage"
    	baseDefinitionName="plmBasePredDynaKitBrowsingPagePrimaryObjectDefinition"
     	objectType="InheritedPredDynaKitBrowsingPage"
     	displayName="${plmPageLayoutResources.inheritedPredDynaKitPage_DisplayName}"
	    headerIcon="inheritedKitPageHeaderIcon" 
	    icon="inheritedKitPageIcon">	
	</PrimaryObjectDefinition>
</Definitions>
