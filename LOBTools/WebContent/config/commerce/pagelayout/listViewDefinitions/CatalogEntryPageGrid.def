<Definitions>

	<!---
		This definition is used to navigate to catalog entry pages from a catalog group page or a product page.
	-->
	<ChildListEditor definitionName="plmCatalogEntryBrowsingPageChildList"
		listDefinitionName="plmCatalogEntryBrowsingPageBrowseGrid"
		objectTypes="ChildCatalogEntryBrowsingPage"/>

	<!---
		This definition displays the contents and characteristics of a list of the following objects:
		{@link plmChildCatalogEntryBrowsingPage}
	-->
	<ObjectGrid definitionName="plmCatalogEntryBrowsingPageBrowseGrid" preferenceKey="plmCatalogEntryBrowsingPageBrowseGrid">
		<GridText editable="false" name="catentryId" objectPath="CatalogEntryBrowsingPageAlias" propertyName="catentryId" text="${catalogResources.productUniqueId_ColumnHeader}" visible="false" width="90"/>
		<GridIconTypeImage name="typeIcon" enableFilter="true" propertyName="null" required="true" text="${catalogResources.productType_ColumnHeader}" visible="true" width="80"/>
		<GridStoreName name="store" objectPath="CatalogEntryBrowsingPageAlias" width="125"/>
		<GridText editable="false" name="ownerId" objectPath="CatalogEntryBrowsingPageAlias" propertyName="ownerId" text="${catalogResources.productOwnerId_ColumnHeader}" visible="false" width="90"/>
		<GridText editable="false" name="partNumber" objectPath="CatalogEntryBrowsingPageAlias" propertyName="partnumber" required="true" text="${plmPageLayoutResources.pageName_ColumnHeader}" visible="true" width="100"/>
		<GridText editable="false" name="catalogEntryName" objectPath="CatalogEntryBrowsingPageAlias" propertyName="name" text="${plmPageLayoutResources.catalogEntryName_ColumnHeader}" visible="true" width="170"/>
		<GridImage name="thumbnailImage" objectPath="CatalogEntryBrowsingPageAlias" propertyName="tImage" sortable="false" text="${catalogResources.productThumbnail_ColumnHeader}" visible="true"/>
		<GridImage name="fullImage" objectPath="CatalogEntryBrowsingPageAlias" propertyName="fImage" sortable="false" text="${catalogResources.productFullImage_ColumnHeader}" visible="false"/>
		<GridText editable ="false" name="sDesc" objectPath="CatalogEntryBrowsingPageAlias" propertyName="sDesc" text="${catalogResources.productShortDesc_ColumnHeader}" visible="false" width="200"/>
	</ObjectGrid>

	<!---
		This definition displays the contents and characteristics of a list of the following objects: {@link ProductBrowsingPage}, {@link KitBrowsingPage}, {@link DynamicKitBrowsingPage}, {@link BundleBrowsingPage}, 
		{@link ProductSKUBrowsingPage} and {@link CatalogGroupSKUBrowsingPage}.
	-->
	<ObjectGrid definitionName="plmCatalogEntryBrowsingPageSearchGrid" preferenceKey="plmCatalogEntryBrowsingPageSearchGrid">
		<GridText editable="false" name="catentryId" propertyName="catentryId" text="${catalogResources.productUniqueId_ColumnHeader}" visible="false" width="90"/>
		<GridIconTypeImage name="typeIcon" propertyName="null" required="true" text="${catalogResources.productType_ColumnHeader}" visible="true" width="60"/>
		<GridStoreName name="store"/>            
		<GridText editable="false" name="ownerId" propertyName="ownerId" text="${catalogResources.productOwnerId_ColumnHeader}" visible="false" width="90"/>
		<GridText editable="false" name="partnumber" propertyName="partnumber" required="true" text="${plmPageLayoutResources.pageName_ColumnHeader}" visible="true" width="90"/>
		<GridText editable="false" name="catalogEntryName" propertyName="name" text="${plmPageLayoutResources.catalogEntryName_ColumnHeader}" visible="true" width="170"/>
		<GridImage name="thumbnailImage"  propertyName="tImage" sortable="false" text="${catalogResources.productThumbnail_ColumnHeader}" visible="true"/>
		<GridImage name="fullImage" propertyName="fImage" sortable="false" text="${catalogResources.productFullImage_ColumnHeader}" visible="false"/>
		<GridText editable ="false" name="sDesc" propertyName="sDesc" text="${catalogResources.productShortDesc_ColumnHeader}" visible="false" width="200"/>
	</ObjectGrid>

	<!---
		This definition displays the identification characteristics of a list of the following objects: {@link ProductBrowsingPage}, {@link KitBrowsingPage}, {@link DynamicKitBrowsingPage}, {@link BundleBrowsingPage}, 
		{@link ProductSKUBrowsingPage} and {@link CatalogGroupSKUBrowsingPage}.  It is used in the utilities pane search tab.
	-->
	<ObjectGrid definitionName="plmCatalogEntryBrowsingPageUtilitiesSearchGrid" preferenceKey="plmCatalogEntryBrowsingPageUtilitiesSearchGrid">
		<GridIconTypeImage name="typeIcon" propertyName="null" required="true" text="${foundationResources.utilitiesGrid_type}" width="100"/>
		<GridDisplayName name="displayName" propertyName="null" showLongDisplayName="true" text="${plmPageLayoutResources.pageName_ColumnHeader}" width="170"/>
		<GridText name="catalogEntryName" propertyName="name" text="${plmPageLayoutResources.catalogEntryName_ColumnHeader}" visible="false" width="170"/>
		<GridImage name="thumbnailImage" propertyName="tImage" sortable="false" text="${catalogResources.productThumbnail_ColumnHeader}" visible="false"/>
	</ObjectGrid>
</Definitions>