<Definitions>

	<!---
		This search is used to find content pages available for the particular store
	 -->
	<SearchDefinition definitionName="FindContentPages" 
		searchType="FindContentPages"
		displayName="${plmPageLayoutResources.findContentPagesSearchDefinition_DisplayName}"
		objectTypeDisplayName="${plmPageLayoutResources.contentPage_DisplayName}"
		icon="storePageIcon"
		listDefinitionName="plmContentPageGrid"
		listTitle="${plmPageLayoutResources.contentPagesSearchTitle}"
		tooltip="${plmPageLayoutResources.searchAllContentPagesTooltip}"
		utilitiesListDefinitionName="plmUtilityContentPageSearchGrid"
		helpLink="concepts/cpzpagecomptool.htm">
			<SearchService name="findContentPages" url="/cmc/FindContentPages">
				<ServiceParam name="storeId"/>
				<ServiceParam name="assetStoreId"/>
			</SearchService>
	</SearchDefinition>
	
</Definitions>