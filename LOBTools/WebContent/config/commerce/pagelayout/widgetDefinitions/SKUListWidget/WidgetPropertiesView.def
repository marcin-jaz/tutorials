<Definitions>

	<GridObjectProperties definitionName="plmWidgetProperties_SKUListWidget">
		<PropertyPane>
			<PropertyGroup name="widgetProperties" collapsable="false" groupTitle="${plmPageLayoutResources.widgetPropertiesPrompt}">
				<PropertyInputText name="${plmPageLayoutResources.widgetNamePrompt}" propertyName="widgetName"
					promptText="${plmPageLayoutResources.widgetNamePrompt}" 
					extendedHelpText="${plmPageLayoutResources.extendedHelpText_WidgetName}" />
				<PropertyCheckbox propertyName="xWidgetProp_disableProductCompare" 
					text="${plmPageLayoutResources.SKUListWidgetShowCompareBoxPrompt}" extendedHelpText="${plmPageLayoutResources.extendedHelpText_SKUListWidgetShowCompareBox}" />					
			</PropertyGroup>
			<PropertyGroup name="contentProperties" collapsable="false" groupTitle="${plmPageLayoutResources.widgetContentPrompt}">
				<PropertyStaticText text="${plmPageLayoutResources.SKUListWidgetContent}" />
			</PropertyGroup>
		</PropertyPane>
	</GridObjectProperties>

</Definitions>
