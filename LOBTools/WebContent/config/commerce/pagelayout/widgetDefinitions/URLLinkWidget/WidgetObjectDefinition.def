<Definitions>
		
	<WidgetObjectDefinition package="plm"
		definitionName="plmLayoutWidget_URLLinkWidget"
		parentDefinitionName="plmBasePageLayoutPrimaryObjectDefinition"
		baseDefinitionName="plmBaseLayoutWidget"
		objectType="URLLinkWidget"
		objectGroups="LayoutWidgetAlias"
		gridPropertiesDefinitionName="plmWidgetProperties_URLLinkWidget"
		iconPath="/images/pagelayouts/widgetIcons/link.png"
		widgetDisplayGroups="AnyPage,CatalogEntryPage,CategoryPage,SearchPage"
		helpLink="concepts/cpzwidlink.htm">
		
		<PropertyDefinition propertyName="xWidgetProp_emsName" copyProtected="true">
		</PropertyDefinition>
		
		<PropertyDefinition propertyName="xWidgetProp_emsId" copyProtected="true">
		</PropertyDefinition>
		
		<CreateService  baseDefinitionName="plmBaseCreateLayoutWidget" />
		<UpdateService baseDefinitionName="plmBaseUpdateLayoutWidget" />	

		<!---
			Retrieves the display title child object {@link plmWidgetDisplayTitle} and {@link plmInheritedWidgetDisplayTitle}.
		-->
		<GetChildrenService objectTypes="WidgetDisplayTitle,InheritedWidgetDisplayTitle" url="/cmc/GetPageLayoutWidgetChildren-DisplayTitle">
			<ServiceParam name="storeId"/>
			<ServiceParam name="pageLayoutId" parentProperty="true" />
			<ServiceParam name="widgetId" propertyName="widgetId"/>
			<ServiceParam name="defaultLanguageId"/>
		</GetChildrenService>

		<!--- Retrieves the child {@link plmURLLinks} and {@link plmInheritedURLLinks} objects. -->
		<GetChildrenService objectTypes="URLLinks,InheritedURLLinks" url="/cmc/GetPageLayoutWidgetChildren-URLLinks">
			<ServiceParam name="storeId"/>
			<ServiceParam name="pageLayoutId" parentProperty="true" />
			<ServiceParam name="widgetId" propertyName="widgetId"/>
		</GetChildrenService>

		<ChildObjectDefinition baseDefinitionName="plmWidgetDisplayTitle"/>
		<ChildObjectDefinition baseDefinitionName="plmInheritedWidgetDisplayTitle"/>
		  		
		<!-- 
			Child object definition for local URL Links.
		 -->
		<ChildObjectDefinition
			baseDefinitionName="plmBaseURLLinks"  
			objectType="URLLinks"
			objectGroups="URLLinksAlias"
			initializeObjectStoreId="true">
			
			<TrueEnablementCondition/>
	
			<!---
				Reference object definition for local content page link.
			-->
			<ReferenceObjectDefinition baseDefinitionName="plmURLLinkContentPageReference"/>
			<!---
				Reference object definition for local catalog group page link.
			-->
			<ReferenceObjectDefinition baseDefinitionName="plmURLLinkCatalogGroupPageReference"/>
			<!---
				Reference object definition for local catalog entry page link.
			-->
			<ReferenceObjectDefinition baseDefinitionName="plmURLLinkCatalogEntryPageReference"/>
			
		</ChildObjectDefinition>
		
		<!-- 
			Child object definition for inherited URL Links.
		 -->
		<ChildObjectDefinition
			baseDefinitionName="plmBaseURLLinks"  
			objectType="InheritedURLLinks"
			allowedParentTypes="InheritedPageLayout"
			objectGroups="URLLinksAlias,AssetStoreObjectsGroup">
			
			<EnablementCondition checkHasAccessRight="true" conditionId="accessRightCondition" enablementValue="true"/>
			
		</ChildObjectDefinition>
		
		<Xml name="template">
			<sequence>0</sequence>
		</Xml>
		
		<MarketingWidgetInheritedObjectValidator package="plm" />
		
	</WidgetObjectDefinition>
	
	<!--  Base child object definition for the URL Links object -->
	<ChildObjectDefinition 
		definitionName="plmBaseURLLinks" 
		isBaseDefinition="true" 
		idProperty="languageId" 
		languageSensitive="true"
		objectGroups="URLLinksAlias" 
		releaseWhenClosed="false">

        <!--- Create service. -->
        <CreateService url="/cmc/CreateWidgetExtendedData">
            <ServiceParam name="storeId"/>
            <ServiceParam name="dataStoreId1" parameterName="dataStoreId" parentProperty="true" resolvePrimaryParent="true" propertyName="objectStoreId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" negate="true" propertyName="objectType"/>
            </ServiceParam>
            <ServiceParam name="dataStoreId2" parameterName="dataStoreId" contextName="storeId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" propertyName="objectType"/>
            </ServiceParam>            
    		<ServiceParam name="pageLayoutId" propertyName="pageLayoutId" parentProperty="true" />
			<ServiceParam name="type" value="IBM_URLLinks"/>
			<ServiceParam name="widgetId" propertyName="widgetId" parentProperty="true" resolvePrimaryParent="false"/>
        	<ServiceParam name="languageId" propertyName="languageId"/>
        </CreateService>
 		
        <!--- Update service. -->
        <UpdateService url="/cmc/UpdateWidgetExtendedData">
            <ServiceParam name="storeId"/>
            <ServiceParam name="dataStoreId1" parameterName="dataStoreId" parentProperty="true" resolvePrimaryParent="true" propertyName="objectStoreId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" negate="true" propertyName="objectType"/>
            </ServiceParam>
            <ServiceParam name="dataStoreId2" parameterName="dataStoreId" contextName="storeId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" propertyName="objectType"/>
            </ServiceParam>            
    		<ServiceParam name="pageLayoutId" propertyName="pageLayoutId" parentProperty="true" />
			<ServiceParam name="type" value="IBM_URLLinks"/>
			<ServiceParam name="widgetId" propertyName="widgetId" parentProperty="true" resolvePrimaryParent="false"/>
        	<ServiceParam name="languageId" propertyName="languageId"/>
        </UpdateService>
		
		<!---
			Child object definition for custom URL link.
		-->
		<ChildObjectDefinition baseDefinitionName="plmURLLinkCustomURL"/>
		<!---
			Reference object definition for inherited content page link.
		-->
		<ReferenceObjectDefinition baseDefinitionName="plmURLLinkInheritedContentPageReference"/>
		<!---
			Reference object definition for inherited catalog group page link.
		-->
		<ReferenceObjectDefinition baseDefinitionName="plmURLLinkInheritedCatalogGroupPageReference"/>
		<!---
			Reference object definition for inherited catalog entry page link.
		-->
		<ReferenceObjectDefinition baseDefinitionName="plmURLLinkInheritedCatalogEntryPageReference"/>

		
	</ChildObjectDefinition>

	<!--- 
		This definition defines the base object from which all URL link page references are derived. 
	-->
	<ReferenceObjectDefinition 
			definitionName="plmBaseURLLinkPageReference"
			isBaseDefinition="true"  
			idProperty="extDataId"
			allowDuplicates="false">

		<!-- Property Definition for the type of the URL link -->
		<PropertyDefinition propertyName="urlType" displayName="${plmPageLayoutResources.urlType}">
			<PropertyValue displayName="${plmPageLayoutResources.customURLType}" value="custom"/>
			<PropertyValue displayName="${plmPageLayoutResources.pageURLType}" value="page"/>
		</PropertyDefinition>

		<!-- Property Definition for the title of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.urlText}" maximumSize="256" propertyName="xExtData_urlText" required="true" trim="true"/>
		<!-- Property Definition for the URL (click action) of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.url}" maximumSize="1024" propertyName="xExtData_url" required="true" trim="true"/>
		<!-- Property Definition for the URL target of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.target}" propertyName="xExtData_target">
			<PropertyValue displayName="${plmPageLayoutResources.openInSameWindow}" value="_self" isDefault="true"/>
			<PropertyValue displayName="${plmPageLayoutResources.openInNewWindow}" value="_blank"/>
		</PropertyDefinition>        
		<!-- Property Definition for the alternate text of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.alternateText}" maximumSize="256" propertyName="xExtData_alt" required="false" trim="true"/>
		<!-- Property Definition for the custom field 1 of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.field1}" maximumSize="256" propertyName="xExtData_xarea_field1" required="false" trim="true"/>
		<!-- Property Definition for the custom field 2 of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.field2}" maximumSize="256" propertyName="xarea_field2" required="false" trim="true"/>
		<!-- Property Definition for the custom field 3 of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.field3}" maximumSize="256" propertyName="xarea_field3" required="false" trim="true"/>
		<!-- Property Definition for the custom field 4 of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.field4}" maximumSize="256" propertyName="xarea_field4" required="false" trim="true"/>
		<!-- Property Definition for the custom field 5 of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.field5}" maximumSize="256" propertyName="xarea_field5" required="false" trim="true"/>
		
        <!--- Create service. -->
        <CreateService url="/cmc/CreateWidgetExtendedData">
            <ServiceParam name="storeId"/>
            <ServiceParam name="dataStoreId1" parameterName="dataStoreId" parentProperty="true" resolvePrimaryParent="true" propertyName="objectStoreId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" negate="true" propertyName="objectType" parentProperty="true" resolvePrimaryParent="false"/>
            </ServiceParam>
            <ServiceParam name="dataStoreId2" parameterName="dataStoreId" contextName="storeId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" propertyName="objectType" parentProperty="true" resolvePrimaryParent="false"/>
            </ServiceParam>            
    		<ServiceParam name="pageLayoutId" propertyName="pageLayoutId" parentProperty="true" />
			<ServiceParam name="type" propertyName="type"/>
			<ServiceParam name="widgetId" propertyName="widgetId" parentProperty="true" objectPath="LayoutWidgetAlias"/>
        	<ServiceParam name="languageId" resolvePrimaryParent="false" parentProperty="true" propertyName="languageId"/>
        </CreateService>
		
        <!--- Update service. -->
        <UpdateService url="/cmc/UpdateWidgetExtendedData">
            <ServiceParam name="storeId"/>
            <ServiceParam name="extDataId" propertyName="extDataId"/>
            <ServiceParam name="dataStoreId1" parameterName="dataStoreId" parentProperty="true" resolvePrimaryParent="true" propertyName="objectStoreId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" negate="true" propertyName="objectType" parentProperty="true" resolvePrimaryParent="false"/>
            </ServiceParam>
            <ServiceParam name="dataStoreId2" parameterName="dataStoreId" contextName="storeId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" propertyName="objectType" parentProperty="true" resolvePrimaryParent="false"/>
            </ServiceParam>            
    		<ServiceParam name="pageLayoutId" propertyName="pageLayoutId" parentProperty="true" />
			<ServiceParam name="type" propertyName="type"/>
			<ServiceParam name="widgetId" propertyName="widgetId" parentProperty="true" objectPath="LayoutWidgetAlias"/>
        	<ServiceParam name="languageId" resolvePrimaryParent="false" parentProperty="true" propertyName="languageId"/>
        	<ServiceParam name="sequenceOrder" propertyName="sequenceOrder"/>
        </UpdateService>
		
        <!--- Delete service. -->
        <DeleteService url="/cmc/DeleteWidgetExtendedData">
            <ServiceParam name="storeId"/>
            <ServiceParam name="extDataId" propertyName="extDataId"/>
            <ServiceParam name="dataStoreId1" parameterName="dataStoreId" parentProperty="true" resolvePrimaryParent="true" propertyName="objectStoreId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" negate="true" propertyName="objectType" parentProperty="true" resolvePrimaryParent="false"/>
            </ServiceParam>
            <ServiceParam name="dataStoreId2" parameterName="dataStoreId" contextName="storeId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" propertyName="objectType" parentProperty="true" resolvePrimaryParent="false"/>
            </ServiceParam>            
    		<ServiceParam name="pageLayoutId" propertyName="pageLayoutId" parentProperty="true" />
			<ServiceParam name="type" propertyName="type"/>
			<ServiceParam name="widgetId" propertyName="widgetId" parentProperty="true" objectPath="LayoutWidgetAlias"/>
        	<ServiceParam name="languageId" resolvePrimaryParent="false" parentProperty="true" propertyName="languageId"/>
        </DeleteService>

		<Xml name="template">
			<urlType>Page</urlType>
			<xExtData_target>_self</xExtData_target>
		</Xml>
	</ReferenceObjectDefinition>	


	<!--- 
		This definition defines the content page reference object. 
	-->
	<ReferenceObjectDefinition 
			definitionName="plmURLLinkContentPageReference" 
			baseDefinitionName="plmBaseURLLinkPageReference" 
			objectType="URLLinkContentPageReference"
			objectGroups="URLLinkContentPageReferenceAlias,URLLinkPageReferenceAlias"
			referencedTypes="ContentPage"
			displayName="${plmPageLayoutResources.contentPageURL}">
		<TrueEnablementCondition/> 
		<Xml name="template">
			<type>IBM_ContentPageURL</type>
		</Xml>		
		
	</ReferenceObjectDefinition>

	<!--- 
		This definition defines the inherited content page reference object. 
	-->
	<ReferenceObjectDefinition 
			definitionName="plmURLLinkInheritedContentPageReference" 
			baseDefinitionName="plmBaseURLLinkPageReference" 
			objectType="URLLinkInheritedContentPageReference"
			objectGroups="URLLinkContentPageReferenceAlias,URLLinkPageReferenceAlias"
			referencedTypes="InheritedContentPage"
			displayName="${plmPageLayoutResources.InheritedContentPageURL}">
		<TrueEnablementCondition/> 
		<Xml name="template">
			<type>IBM_ContentPageURL</type>
		</Xml>			
	</ReferenceObjectDefinition>

	<!--- 
		This definition defines the category page reference object. 
	-->
	<ReferenceObjectDefinition 
			definitionName="plmURLLinkCatalogGroupPageReference" 
			baseDefinitionName="plmBaseURLLinkPageReference" 
			objectType="URLLinkCatalogGroupPageReference"
			objectGroups="URLLinkCatalogGroupPageReferenceAlias,URLLinkPageReferenceAlias"
			referencedTypes="CatalogGroupBrowsingPage"
			displayName="${plmPageLayoutResources.catalogGroupPageURL}">
		<TrueEnablementCondition/> 
		<Xml name="template">
			<type>IBM_CatalogGroupPageURL</type>
		</Xml>		
	</ReferenceObjectDefinition>

	<!--- 
		This definition defines the inherited category page reference object. 
	-->
	<ReferenceObjectDefinition 
			definitionName="plmURLLinkInheritedCatalogGroupPageReference" 
			baseDefinitionName="plmBaseURLLinkPageReference" 
			objectType="URLLinkInheritedCatalogGroupPageReference"
			objectGroups="URLLinkCatalogGroupPageReferenceAlias,URLLinkPageReferenceAlias"
			referencedTypes="InheritedCatalogGroupBrowsingPage"
			displayName="${plmPageLayoutResources.InheritedCatalogGroupPageURL}">
		<TrueEnablementCondition/> 
		<Xml name="template">
			<type>IBM_CatalogGroupPageURL</type>
		</Xml>			
	</ReferenceObjectDefinition>


	<!--- 
		This definition defines the catalog entry page reference object. 
	-->
	<ReferenceObjectDefinition 
			definitionName="plmURLLinkCatalogEntryPageReference" 
			baseDefinitionName="plmBaseURLLinkPageReference" 
			objectType="URLLinkCatalogEntryPageReference"
			objectGroups="URLLinkCatalogEntryPageReferenceAlias,URLLinkPageReferenceAlias"
			referencedTypes="ProductBrowsingPage,BundleBrowsingPage,KitBrowsingPage,DynamicKitBrowsingPage,PredDynaKitBrowsingPage,CatalogGroupSKUBrowsingPage,ProductSKUBrowsingPage"
			displayName="${plmPageLayoutResources.catalogEntryPageURL}">
		<TrueEnablementCondition/> 
		<Xml name="template">
			<type>IBM_CatalogEntryPageURL</type>
		</Xml>		
		
	</ReferenceObjectDefinition>

	<!--- 
		This definition defines the inherited catalog entry page reference object. 
	-->
	<ReferenceObjectDefinition 
			definitionName="plmURLLinkInheritedCatalogEntryPageReference" 
			baseDefinitionName="plmBaseURLLinkPageReference" 
			objectType="URLLinkInheritedCatalogEntryPageReference"
			objectGroups="URLLinkCatalogEntryPageReferenceAlias,URLLinkPageReferenceAlias"
			referencedTypes="InheritedProductBrowsingPage,InheritedBundleBrowsingPage,InheritedKitBrowsingPage,InheritedDynamicKitBrowsingPage,InheritedPredDynaKitBrowsingPage,InheritedCatalogGroupSKUBrowsingPage,InheritedProductSKUBrowsingPage"
			displayName="${plmPageLayoutResources.InheritedCatalogEntryPageURL}">
		<TrueEnablementCondition/> 
		<Xml name="template">
			<type>IBM_CatalogEntryPageURL</type>
		</Xml>			
	</ReferenceObjectDefinition>

	<!--- 
		This definition defines the custom URL child object 
	-->
	<ChildObjectDefinition 
			definitionName="plmURLLinkCustomURL"
			objectType="URLLinkCustomURL"
			idProperty="extDataId"
			displayName="${plmPageLayoutResources.customURL}"
			icon="URLIcon">
		<!-- Property Definition for the type of the URL link -->
		<PropertyDefinition propertyName="urlType" displayName="${plmPageLayoutResources.urlType}">
			<PropertyValue displayName="${plmPageLayoutResources.customURLType}" value="custom"/>
			<PropertyValue displayName="${plmPageLayoutResources.pageURLType}" value="page"/>
		</PropertyDefinition>
			
		<!-- Property Definition for the title of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.urlText}" maximumSize="256" propertyName="xExtData_urlText" required="true" trim="true"/>
		<!-- Property Definition for the URL (click action) of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.url}" maximumSize="1024" propertyName="xExtData_url" required="true" trim="true"/>
		<!-- Property Definition for the URL target of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.target}" propertyName="xExtData_target">
			<PropertyValue displayName="${plmPageLayoutResources.openInSameWindow}" value="_self" isDefault="true"/>
			<PropertyValue displayName="${plmPageLayoutResources.openInNewWindow}" value="_blank"/>
		</PropertyDefinition>        
		<!-- Property Definition for the alternate text of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.alternateText}" maximumSize="256" propertyName="xExtData_alt" required="false" trim="true"/>
		<!-- Property Definition for the custom field 1 of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.field1}" maximumSize="256" propertyName="xExtData_field1" required="false" trim="true"/>
		<!-- Property Definition for the custom field 2 of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.field2}" maximumSize="256" propertyName="xExtData_field2" required="false" trim="true"/>
		<!-- Property Definition for the custom field 3 of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.field3}" maximumSize="256" propertyName="xExtData_field3" required="false" trim="true"/>
		<!-- Property Definition for the custom field 4 of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.field4}" maximumSize="256" propertyName="xExtData_field4" required="false" trim="true"/>
		<!-- Property Definition for the custom field 5 of the link -->
		<PropertyDefinition displayName="${plmPageLayoutResources.field5}" maximumSize="256" propertyName="xExtData_field5" required="false" trim="true"/>
		
		
        <!--- Create service. -->
        <CreateService url="/cmc/CreateWidgetExtendedData">
            <ServiceParam name="storeId"/>
            <ServiceParam name="dataStoreId1" parameterName="dataStoreId" parentProperty="true" resolvePrimaryParent="true" propertyName="objectStoreId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" negate="true" propertyName="objectType" parentProperty="true" resolvePrimaryParent="false"/>
            </ServiceParam>
            <ServiceParam name="dataStoreId2" parameterName="dataStoreId" contextName="storeId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" propertyName="objectType" parentProperty="true" resolvePrimaryParent="false"/>
            </ServiceParam>            
    		<ServiceParam name="pageLayoutId" propertyName="pageLayoutId" parentProperty="true" />
			<ServiceParam name="type" value="IBM_CustomURL"/>
			<ServiceParam name="widgetId" propertyName="widgetId" parentProperty="true" objectPath="LayoutWidgetAlias"/>
        	<ServiceParam name="languageId" resolvePrimaryParent="false" parentProperty="true" propertyName="languageId"/>
        </CreateService>

        <!--- Update service. -->
        <UpdateService url="/cmc/UpdateWidgetExtendedData">
            <ServiceParam name="storeId"/>
            <ServiceParam name="extDataId" propertyName="extDataId"/>
            <ServiceParam name="dataStoreId1" parameterName="dataStoreId" parentProperty="true" resolvePrimaryParent="true" propertyName="objectStoreId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" negate="true" propertyName="objectType" parentProperty="true" resolvePrimaryParent="false"/>
            </ServiceParam>
            <ServiceParam name="dataStoreId2" parameterName="dataStoreId" contextName="storeId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" propertyName="objectType" parentProperty="true" resolvePrimaryParent="false"/>
            </ServiceParam>            
    		<ServiceParam name="pageLayoutId" propertyName="pageLayoutId" parentProperty="true" />
			<ServiceParam name="type" value="IBM_CustomURL"/>
			<ServiceParam name="widgetId" propertyName="widgetId" parentProperty="true" objectPath="LayoutWidgetAlias"/>
        	<ServiceParam name="languageId" resolvePrimaryParent="false" parentProperty="true" propertyName="languageId"/>
        	<ServiceParam name="sequenceOrder" propertyName="sequenceOrder"/>
        </UpdateService>
		
        <!--- Delete service. -->
        <DeleteService url="/cmc/DeleteWidgetExtendedData">
            <ServiceParam name="storeId"/>
            <ServiceParam name="extDataId" propertyName="extDataId"/>
            <ServiceParam name="dataStoreId1" parameterName="dataStoreId" parentProperty="true" resolvePrimaryParent="true" propertyName="objectStoreId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" negate="true" propertyName="objectType" parentProperty="true" resolvePrimaryParent="false"/>
            </ServiceParam>
            <ServiceParam name="dataStoreId2" parameterName="dataStoreId" contextName="storeId">
            	<EnablementCondition checkObjectDefinition="true" conditionId="objectTypeCondition" enablementValue="URLLinks" propertyName="objectType" parentProperty="true" resolvePrimaryParent="false"/>
            </ServiceParam>            
    		<ServiceParam name="pageLayoutId" propertyName="pageLayoutId" parentProperty="true" />
			<ServiceParam name="type" value="IBM_CustomURL"/>
			<ServiceParam name="widgetId" propertyName="widgetId" parentProperty="true" objectPath="LayoutWidgetAlias"/>
        	<ServiceParam name="languageId" resolvePrimaryParent="false" parentProperty="true" propertyName="languageId"/>
        </DeleteService>

		<Xml name="template">
			<urlType>custom</urlType>
			<xExtData_target>_self</xExtData_target>
		</Xml>
	</ChildObjectDefinition>

</Definitions>
