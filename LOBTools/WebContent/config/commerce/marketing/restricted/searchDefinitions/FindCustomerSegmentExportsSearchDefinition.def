<Definitions>
	<!--
		This search is used to find {@link CustomerSegmentExport CustomerSegmentExport}. 
		The search retrieves objects that match the search term. The search term text 
		is compared with the values of the following property: 
		<ul> 
			<li>fileName - this is the name of the customer segment customer information export task.</li> 
			<li>customerSegmentName - this is the name of the customer segment whose customer information are exported.</li> 
		</ul>
	-->
	<SearchDefinition definitionName="FindCustomerSegmentExports"
		displayName="${mktMarketingResources.customerSegmentExportObjectName}"
		headerText="${mktMarketingResources.customerSegmentExport_name}"
		icon="customerSegmentExportIcon"
		listDefinitionName="mktCustomerSegmentExportGrid"
		listTitle="${mktMarketingResources.customerSegmentExportSearchResultTitle}"
		searchType="FindCustomerSegmentExports" tooltip="${mktMarketingResources.searchCustomerSegmentExportsTooltip}">
		<SearchService name="findCustomerSegmentExports" url="/cmc/FindCustomerSegmentExports">
			<ServiceParam name="storeId" />
		</SearchService>
	</SearchDefinition>
</Definitions>